{
  "name": "a11y-date-input",
  "version": "0.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:react": "turbo run dev --filter=react-app...",
    "dev:angular": "turbo run dev --filter=angular-app...",
    "dev:solid": "turbo run dev --filter=solid-app...",
    "dev:svelte": "turbo run dev --filter=svelte-app...",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,md,html}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,css,md,html}\"",
    "clean": "turbo run clean",
    "type-check": "turbo run type-check",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "turbo": "^2.3.3",
    "typescript": "~5.8.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18"
  }
}
